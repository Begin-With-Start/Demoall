<?xml version="1.0" encoding="UTF-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!--  设置背景色-->
    <item  android:id="@android:id/background" >
        <shape>
            <solid android:color="@color/common_color_upload_progress_21000000"/>
        </shape>
    </item>

    <!--  设置进度条颜色 -->
    <item  android:id="@android:id/progress" >
        <clip>
            <shape>
                <solid android:color="@color/common_color_c1_0971ce"/>
            </shape>
        </clip>
    </item>

</layer-list>